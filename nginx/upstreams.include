# Shared upstream definitions for Splunk Demo

upstream queue_manager {
    server queue-manager:3000;
}

upstream terminal {
    server queue-manager:7681;
}

upstream grafana {
    server lgtm:3000;
}

upstream splunk_web {
    server splunk:8000;
}

upstream webhook_catcher {
    server webhook-catcher:8080;
}
